    <div class="container main">
      <div class="col-sm-12">
        <h2 class="banner-client">Porter Novelli Retirement Game Microsite</h2>
        <h3 class="banner-name">Pages</h3>
        <div class="table-responsive">
          <table class="table">
            <tbody>
              {{#each pages }}
                <tr>
                  <td>
                    <a class="banner-size" href="{{ this.page_url }}" target="_blank">{{ this.name }}</a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>

