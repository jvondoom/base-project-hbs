    <style>

      header {
        background-color: #ED2924;
        height: 600px;
      }

      h2 {
        font-family: DIN-Black, sans-serif;
        color: #ED2924;
        font-size: 26px;
        font-weight: 600;
      }

      a {
        color: #000!important;
        font-size: 15px;
      }

      table>tbody>tr>td {
        padding: 27px 3px!important;
        border-top: 1px solid #818181 !important;
      }

      .main {
        margin-top: -350px;
        background-color: #FFF;
        width: 1025px !important;
      }

      .banner-client {
        margin-top: 60px;
      }

      .banner-name {
        font-family: DIN-Medium, sans-serif;
        border-top: 1px solid #FFF;
        font-weight: 300;
        margin-top: 46px;
        margin-bottom: 0;
        padding-bottom: 17px;
        border-bottom: 1px solid #000!important;
      }

      .banner-size {
        font-family: DIN-Regular, sans-serif;
        font-size: 15px;
      }

      .zip {
        font-family: DIN-Bold, sans-serif;
        font-size: 15px;
      }

      .logo-wrap {
        display: block;
        margin: 0 auto;
        height: 93px;
        width: 367px;
        padding: 80px 0;
      }

      #logo {
        height: 93px;
        width: 367px;
      }

      #footer-logo {
        float: right;
        height: 36px;
        width: 35px;
      }

      .h-m-footer {
        margin-top: 100px;
        margin-bottom: 43px;
      }

      .h-m-footer p {
        color: #999999;
        font-size: 7px;
        line-height: 30px;
      }

      .error * {
        color: #ED2924 !important;
      }
    </style>
  </head>

